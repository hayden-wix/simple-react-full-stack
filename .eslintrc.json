{
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "object-curly-spacing": "off",
    "react/prop-types": 0,
    "indent": "off",
    "no-console": "off",
    "comma-dangle": "off",
    "react/jsx-filename-extension": "off",
    "react/jsx-tag-spacing": "off",
    "react/jsx-closing-bracket-location": "off"
  }
}
